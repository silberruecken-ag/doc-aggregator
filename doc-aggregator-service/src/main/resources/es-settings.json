{
  "analysis": {
    "analyzer": {
      "html_analyzer": {
        "type": "custom",
        "char_filter": [
          "html_strip"
        ],
        "tokenizer": "standard",
        "filter": [
          "lowercase",
          "stop",
          "snowBallEnglish"
        ]
      }
    },
    "filter": {
      "snowBallEnglish": {
        "type": "snowball",
        "language": "English"
      }
    }
  }
}
